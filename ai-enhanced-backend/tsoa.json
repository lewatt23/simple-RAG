{
  "entryFile": "src/index.ts",
  "noImplicitAdditionalProperties": "throw-on-extras",
  "controllerPathGlobs": ["./src/**/*Controller.ts"],


  "ignore": [
    "**/node_modules/**"
  ],

  "spec": {
    "specVersion": 3,
    "host": null,
    "outputDirectory": "./src/routes"
  },
  "routes": {
    "basePath": "",
    "routesDir": "./src/routes",
    "middleware": "express"
  },

  "multerOpts": {
    "limits": {
      "fileSize": 314572800
    }
  }
}
